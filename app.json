{
  "expo": {
    "name": "pixel-class",
    "slug": "pixel-class",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://ik.imagekit.io/pxc/pixel%20class%20fav%20w-02.png",
    "scheme": "pixelclass",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "https://ik.imagekit.io/pxc/pixel%20class%20fav%20w-02.png",
        "backgroundImage": "https://ik.imagekit.io/pxc/pixel%20class%20fav%20w-02.png",
        "monochromeImage": "https://ik.imagekit.io/pxc/pixel%20class%20fav%20w-02.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "https://ik.imagekit.io/pxc/pixel%20class%20fav%20w-02.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "https://ik.imagekit.io/pxc/pixel%20class%20fav%20w-02.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}