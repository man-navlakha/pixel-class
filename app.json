{
  "expo": {
    "name": "Pixel Class",
    "slug": "pixelclass",
    "version": "1.1.6",
    "orientation": "portrait",
    "icon": "https://ik.imagekit.io/pxc/pixel%20class%20fav%20w-02.png",
    "scheme": "pixelclass",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mannavlakha.pixelclass",
      "buildNumber": "1.1.6"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "https://ik.imagekit.io/pxc/pixel%20class%20fav%20w-02.png",
        "backgroundImage": "https://ik.imagekit.io/pxc/pixel%20class%20fav%20w-02.png",
        "monochromeImage": "https://ik.imagekit.io/pxc/pixel%20class%20fav%20w-02.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "pixelclass"
            },
            {
              "scheme": "https",
              "host": "pixelclass.netlify.app",
              "pathPrefix": "/auth/password"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.mannavlakha.pixelclass",
      "versionCode": 116
    },
    "web": {
      "output": "static",
      "favicon": "https://ik.imagekit.io/pxc/pixel%20class%20fav%20w-02.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "https://ik.imagekit.io/pxc/pixel%20class%20fav%20w-02.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-notifications",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d2ea2d66-46a8-4cae-ae33-5f117a0ec481"
      }
    }
  }
}